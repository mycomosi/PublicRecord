{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "source": {
    "include": [
      "src/es6/",
      "./README.md"
    ]
  },
  "sourceType": "module",
  "opts": {
    "destination": "./doc/",
		"template": "node_modules/docdash"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
